{
    "name": "cordon/account-review",
    "description": "Un bundle Symfony pour la revue des comptes utilisateurs",
    "type": "symfony-bundle",
    "version": "2.0.0",
    "autoload": {
        "psr-4": {
            "Cordon\\AccountReview\\": "src/"
